	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "attach",
			"name": "Docker: Attach to orchestrator",
			"address": "localhost",
			"port": 9230,
			"localRoot": "${workspaceFolder}/apps/orchestrator",
			"remoteRoot": "/app/apps/orchestrator",
			"restart": true,
			"skipFiles": [
				"<node_internals>/**"
			]
		},
		{
			"type": "node",
			"request": "attach",
			"name": "Docker: Attach to email-int",
			"address": "localhost",
			"port": 9230,
			"localRoot": "${workspaceFolder}/apps/email-int",
			"remoteRoot": "/app/apps/email-int",
			"restart": true,
			"skipFiles": [
				"<node_internals>/**"
			]
		},
	]
}